{
  "name": "just-queue",
  "version": "1.0.0",
  "author": "nonAlgebraic <github.com/nonAlgebraic>",
  "license": "MIT",
  "scripts": {
    "start": "concurrently 'tsc -b tsconfig.build.json -w' 'servor public --reload'",
    "build": "tsc -b tsconfig.build.json",
    "test": "jest",
    "format": "eslint --fix src/**/*.tsx && stylelint --fix src/**/*.tsx",
    "lint": "eslint src/**/*.tsx && stylelint src/**/*.tsx",
    "clean": "git clean -Xf public",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "emotion": "*",
    "preact": "*",
    "xstate": "*"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "*",
    "@graphql-codegen/introspection": "*",
    "@graphql-codegen/typescript": "*",
    "@graphql-codegen/typescript-operations": "*",
    "@testing-library/jest-dom": "*",
    "@testing-library/preact": "*",
    "@types/jest": "*",
    "@types/jest-axe": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "concurrently": "*",
    "eslint": "*",
    "eslint-config-preact": "*",
    "eslint-config-prettier": "*",
    "eslint-config-standard-with-typescript": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jest": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-node": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-standard": "*",
    "graphql": "",
    "jest": "*",
    "jest-axe": "*",
    "jest-emotion": "*",
    "prettier": "*",
    "servor": "*",
    "stylelint": "*",
    "stylelint-config-prettier": "*",
    "stylelint-config-rational-order": "*",
    "stylelint-config-standard": "*",
    "stylelint-config-styled-components": "*",
    "stylelint-order": "*",
    "stylelint-prettier": "*",
    "ts-jest": "*",
    "typescript": "*"
  }
}
