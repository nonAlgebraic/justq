    "format": "eslint --fix src/**/*.tsx && stylelint --fix src/**/*.tsx",
    "lint": "eslint src/**/*.tsx && stylelint src/**/*.tsx",
    "clean": "git clean -Xf public",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config"
  },
