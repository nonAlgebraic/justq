    "eslint-plugin-prettier": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-standard": "*",
    "graphql": "*",
    "jest": "*",
    "jest-axe": "*",
    "jest-emotion": "*",
    "prettier": "*",
    "react": "*",
    "servor": "*",
