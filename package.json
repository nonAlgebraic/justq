    "@graphql-codegen/typescript-graphql-request": "*",
    "@graphql-codegen/typescript-operations": "*",
    "@graphql-codegen/typescript-resolvers": "*",
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@types/jest": "*",
    "@types/jest-axe": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@typescript-eslint/eslint-plugin": "*",
